{
  "productName": "EncodingMan",
  "version": "1.2.0",
  "identifier": "com.encodingman.csvtool",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "EncodingMan - 文字化け修正ツール",
        "width": 640,
        "height": 480,
        "resizable": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ],
    "windows": {
      "nsis": {
        "displayLanguageSelector": true,
        "languages": ["Japanese", "English"]
      }
    },
    "fileAssociations": [
      {
        "ext": ["csv"],
        "mimeType": "text/csv",
        "name": "CSV File",
        "role": "Editor"
      },
      {
        "ext": ["tsv"],
        "mimeType": "text/tab-separated-values",
        "name": "TSV File",
        "role": "Editor"
      },
      {
        "ext": ["txt"],
        "mimeType": "text/plain",
        "name": "Text File",
        "role": "Editor"
      },
      {
        "ext": ["dat"],
        "mimeType": "application/octet-stream",
        "name": "Data File",
        "role": "Editor"
      },
      {
        "ext": ["log"],
        "mimeType": "text/plain",
        "name": "Log File",
        "role": "Editor"
      },
      {
        "ext": ["xsl", "xslt"],
        "mimeType": "application/xml",
        "name": "XSL File",
        "role": "Editor"
      },
      {
        "ext": ["xml"],
        "mimeType": "application/xml",
        "name": "XML File",
        "role": "Editor"
      },
      {
        "ext": ["json"],
        "mimeType": "application/json",
        "name": "JSON File",
        "role": "Editor"
      },
      {
        "ext": ["htm", "html"],
        "mimeType": "text/html",
        "name": "HTML File",
        "role": "Editor"
      },
      {
        "ext": ["xls"],
        "mimeType": "application/vnd.ms-excel",
        "name": "Excel File (xls)",
        "role": "Editor"
      },
      {
        "ext": ["xlsx", "xlsm", "xlsb"],
        "mimeType": "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet",
        "name": "Excel File (xlsx)",
        "role": "Editor"
      },
      {
        "ext": ["doc"],
        "mimeType": "application/msword",
        "name": "Word File (doc)",
        "role": "Editor"
      },
      {
        "ext": ["docx", "docm"],
        "mimeType": "application/vnd.openxmlformats-officedocument.wordprocessingml.document",
        "name": "Word File (docx)",
        "role": "Editor"
      },
      {
        "ext": ["ppt"],
        "mimeType": "application/vnd.ms-powerpoint",
        "name": "PowerPoint File (ppt)",
        "role": "Editor"
      },
      {
        "ext": ["pptx", "pptm"],
        "mimeType": "application/vnd.openxmlformats-officedocument.presentationml.presentation",
        "name": "PowerPoint File (pptx)",
        "role": "Editor"
      },
      {
        "ext": ["pdf"],
        "mimeType": "application/pdf",
        "name": "PDF File",
        "role": "Editor"
      }
    ]
  }
}
